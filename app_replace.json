{
  "reddit-to-apollo": {
    "name": "Reddit to Apollo for Reddit",
    "mappings": [
      {
        "from": "https:\/\/((www\\.)?reddit\\.com.*)",
        "to": "apollo://$0"
      },
      {
        "from": "reddit:\/\/(.*)",
        "to": "apollo://$0"
      }
    ]
  },
  "twitter-to-tweetbot": {
    "name": "Twitter to Tweetbot",
    "mappings": [
      {
        "from": "https:\/\/((www\\.|mobile\\.)?twitter\\.com\/?)$",
        "to": "tweetbot://$0"
      },
      {
        "from": "https:\/\/(www\\.|mobile\\.)?twitter\\.com\/([^\/]*)$",
        "to": "tweetbot://$0"
      },
      {
        "from": "https:\/\/(www\\.|mobile\\.)?twitter\\.com\/(.*)\/status\/(\\d*)",
        "to": "tweetbot://$0"
      }
    ]
  }
}
